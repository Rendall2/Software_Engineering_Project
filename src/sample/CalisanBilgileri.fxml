<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CalisanBilgileriController">
   <children>
      <Button layoutX="14.0" layoutY="420.0" mnemonicParsing="false" onAction="#geriButtonPushed" text="Geri" />
      <VBox layoutX="47.0" layoutY="96.0" prefHeight="263.0" prefWidth="515.0" spacing="15.0">
         <children>
            <TableView fx:id="CalisanTableView" prefHeight="280.0" prefWidth="515.0">
              <columns>
                <TableColumn fx:id="CalisanAdiTableColumn" onEditCommit="#changeAdCellEvent" prefWidth="144.79999542236328" text="Ad" />
                <TableColumn fx:id="CalisanSoyadiTableColumn" onEditCommit="#changeSoyadCellEvent" prefWidth="136.79998779296875" text="Soyad" />
                  <TableColumn fx:id="CalisanLevelTableColumn" onEditCommit="#changeLevelCellEvent" prefWidth="102.4000244140625" text="Level" />
                  <TableColumn fx:id="CalisanSertifikaTarihiTableColumn" onEditCommit="#changeSertifikaTarihi" prefWidth="129.59999389648442" text="Sertifika Bitiş Süresi" />
              </columns>
            </TableView>
            <HBox prefHeight="25.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <TextField fx:id="adTextField" prefWidth="130.0" promptText="Ad" />
                  <TextField fx:id="soyadTextField" prefHeight="26.0" prefWidth="110.0" promptText="Soyad" />
                  <TextField fx:id="levelTextField" prefHeight="26.0" prefWidth="105.0" promptText="Level" />
                  <DatePicker fx:id="sertifikaTarihiDatePicker" prefHeight="26.0" prefWidth="150.0" promptText="Sertifika Bitiş Tarihi" />
               </children></HBox>
            <HBox prefHeight="25.6" prefWidth="515.0">
               <children>
                  <HBox prefHeight="26.0" prefWidth="207.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#calisanSil" prefHeight="26.0" prefWidth="144.0" text="Seçilen Çalışan(lar)ı Sil" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" prefHeight="26.0" prefWidth="317.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#calisanEkle" text="Yeni Çalışan Ekle" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Label alignment="CENTER" prefHeight="68.0" prefWidth="610.0" style="-fx-background-color: #000000;" text="GÖZETİM MUAYENE VE  EĞİTİM HİZMETLERİ" textFill="#dacfcf">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>
      <Label fx:id="errorMsgLabel" layoutX="47.0" layoutY="383.0" prefHeight="26.0" prefWidth="515.0" text="Label" textFill="RED">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
